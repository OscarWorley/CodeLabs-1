#include <iostream>//standard namespace
using namespace std;//Main function

int main()//Main function
{
    for (int i = 50; i >= 10 ; i -= 2) {//Declaring the i as an integer. Then making the for loop repeats until it reaches 10 /when the condition is met.
        cout << i << "\n";//outputing and displaying each integer with the increament of -2
    }

}
