#include <iostream>//standard namespace
using namespace std;//Main function

int main()//Main function
{
    for (int i = 30; i <= 50 ; i++) {//Declaring the i as an integer. Then making the for loop repeats until it reaches 50 /when the condition is met.
        cout << i << "\n";//outputing and displaying each integer with the increament of +1
    }

}
